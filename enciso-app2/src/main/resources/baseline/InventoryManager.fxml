      <TextField fx:id="dollarField" layoutX="432.0" layoutY="133.0" promptText="Enter new Dollar amount" />
      <TextField fx:id="serialField" layoutX="432.0" layoutY="183.0" promptText="Enter new Serial Number " />
      <Label layoutX="432.0" layoutY="66.0" text="Item Name" />
      <Label layoutX="432.0" layoutY="116.0" text="Dollar Amount" />
      <Label layoutX="433.0" layoutY="166.0" text="Serial Number" />
      <Button layoutX="432.0" layoutY="231.0" mnemonicParsing="false" onAction="#addItemButtonClicked" prefHeight="25.0" prefWidth="149.0" text="Add Item" />
      <Button layoutX="432.0" layoutY="281.0" mnemonicParsing="false" onAction="#removeItemButtonClicked" prefHeight="25.0" prefWidth="149.0" text="Remove Item" />
      <Button layoutX="432.0" layoutY="327.0" mnemonicParsing="false" onAction="#removeAllButtonClicked" prefHeight="25.0" prefWidth="149.0" text="Remove All" />
      <Button layoutX="432.0" layoutY="391.0" mnemonicParsing="false" onAction="#helpButtonClicked" prefHeight="25.0" prefWidth="149.0" text="Help!" />
   </children>
</AnchorPane>
