      <TableView fx:id="table" editable="true" layoutX="28.0" layoutY="54.0" onMouseClicked="#getSelected" prefHeight="298.0" prefWidth="385.0">
        <columns>
          <TableColumn fx:id="nameCol" prefWidth="75.0" text="Item Name" />
          <TableColumn fx:id="dollarCol" prefWidth="75.0" text="Dollar Value" />
            <TableColumn fx:id="serialCol" prefWidth="75.0" text="Serial Number" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <TextField fx:id="searchField" layoutX="68.0" layoutY="14.0" prefHeight="25.0" prefWidth="345.0" promptText="Search by Name, Value, or Serial" />
      <Label layoutX="28.0" layoutY="14.0" prefHeight="25.0" prefWidth="51.0" text="Search" />
      <Button layoutX="486.0" layoutY="14.0" mnemonicParsing="false" onAction="#saveButtonClicked" text="Save" />
      <Button layoutX="547.0" layoutY="14.0" mnemonicParsing="false" onAction="#loadButtonClicked" text="Load" />
      <TextField fx:id="statusField" layoutX="28.0" layoutY="369.0" prefHeight="69.0" prefWidth="385.0" />
